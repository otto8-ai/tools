Name: Google Sheets OAuth Credential
Share Credential: ../../../oauth2 as google.sheets.write
    with SHEETS_GOOGLE_OAUTH_TOKEN as env and
        google-sheets as integration and
        "https://www.googleapis.com/auth/userinfo.email
        openid
        https://www.googleapis.com/auth/userinfo.profile
        https://www.googleapis.com/auth/spreadsheets
        https://www.googleapis.com/auth/drive.metadata.readonly" as scope
Type: credential
